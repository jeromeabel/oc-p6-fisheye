{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-undef": "error",
        "eqeqeq": "warn",
        "no-unused-vars": "error",
        "indent": [
            "warn",
            2,
            {
                "SwitchCase": 1
            }
        ],
        "multiline-ternary": [
            "warn",
            "always-multiline"
        ]
    }
}